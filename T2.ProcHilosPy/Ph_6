from multiprocessing import Process
import os

def info(titulo):
  print(titulo)
  print('module name: ', __name__)
  print('parent process: ', os.getppid())
  print('process id: ', os.getpid())

def f(nombre):
  info('Funci√≥n f')
  print('Hello', nombre)
  print("---------")

#f __name__ == '__main__':

info('Primera linea')
p = Process(target=f, args=('Laz',))
p.start()
p.join()
